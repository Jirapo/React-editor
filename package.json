{
  "name": "ele-demo",
  "version": "1.0.0",
  "description": "an simple react IDE, made by electron",
  "main": "./src/main/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron ./src/main/index.js",
    "start-build": "cross-env NODE_ENV=production electron ./src/main/index.js",
    "server": "node server.js --port 3000 ",
    "server-user": "node server.js --port 3001",
    "build": "./node_modules/.bin/webpack --config webpack/prod.config.js --display-error-details --color",
    "build-user": "./node_modules/.bin/webpack --config user.webpack.config.prod.js  --display-error-details --color",
    "watch": "webpack-dev-server --config webpack/dev.config.js --watch",
    "watch-user": "webpack-dev-server --config user.webpack.config.js --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "jirapo",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "babel": "^6.5.2",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015-webpack": "^6.4.3",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-standalone": "^6.21.1-0",
    "brace": "^0.9.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "del": "^2.2.2",
    "dva": "^1.1.0",
    "electron": "^1.4.12",
    "electron-default-menu": "^1.0.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-inline-chunk-plugin": "^1.0.0",
    "html-webpack-plugin": "^2.24.1",
    "node-dir": "^0.1.16",
    "path": "^0.12.7",
    "postcss-loader": "^1.2.1",
    "react": "^15.4.1",
    "react-ace": "^4.1.0",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
